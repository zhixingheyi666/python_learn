import B_to_import as bti

#yao = bti.lili( love )

def love( male, female ):
    print( male, 'will love ', female, 'forever!!!' )

yao = bti.lili( love )

class wq( object ):
    def __init__( self ):
        self.female = 'shenlili'
    def saylove( self ):
        print( 'invoke saylove' )
        yao.make( self.female )

wish = wq()
wish.saylove()
