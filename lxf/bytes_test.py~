b0 = b'\x81\x81\x52\x01\x00\x00\x53\01'

b1 = b'\x14\x03\x00\x00\x00\x02\xc6\xce'

for i in b0:
    print( i )
print( b0 + b1 )
print( b0[::2] )
for i in range( 7 ):
    print( b0[:i] )
print( 'test head cut!' )

print( b1 )
b1 = b1[5:]
print( b1 )

print( 'Test in' )
print( 129  in b0 )
print( b0[2] in range(82) )
print( b0[2] in range(81,83) )

print( 'Caculate Test!' )
print( b0[0] - 128 )
